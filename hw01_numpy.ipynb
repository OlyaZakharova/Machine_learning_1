      "source": [
        "def construct_matrix(m: int, a: int, b: int) -> np.array:\n",
        "    new_str = np.arange(a,b+1)\n",
        "    dubl_str = np.tile(new_str,m)\n",
        "    return dubl_str.reshape(m, b - a +1)"
      ]
    },
