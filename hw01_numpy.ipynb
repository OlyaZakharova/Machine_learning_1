      "outputs": [],
      "source": [
        "def cosine_similarity(vec1: np.array, vec2: np.array) -> float:\n",
        "    dot_product = np.sum(vec1 * vec2)\n",
        "\n",
        "    norms_vec1 = np.sqrt(np.sum(vec1 **2))\n",
        "    norms_vec2 = np.sqrt(np.sum(vec2 **2))\n",
        "\n",
        "    return dot_product/(norms_vec1 * norms_vec2)"
      ]
    },
