      "source": [
        "def make_symmetric(matrix: np.array) -> np.array:\n",
        "    a = matrix.transpose()\n",
        "    b = np.diag(a)\n",
        "    a_new = np.where(a == b, 0, a)\n",
        "    return matrix + a_new"
      ]
    },
