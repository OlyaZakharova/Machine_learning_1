    {
      "cell_type": "code",
      "source": [
        "def pervoobrazn(coeffs, constant):\n",
        "  n = len(coeffs)\n",
        "  powers = np.arange(n-1, -1, -1)\n",
        "  new_coeffs = coeffs / (powers +1)\n",
        "  new_coeffs = np.append(new_coeffs, constant)\n",
        "  return new_coeffs"
      ],
